{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-nyc-payroll"
		},
		"ls_adls_masterdata_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_masterdata'"
		},
		"ls_sqldb_nycpayroll_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldb_nycpayroll'"
		},
		"ls_synapse_udacitynycpayroll_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_synapse_udacitynycpayroll'"
		},
		"ls_adls_masterdata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayroll.dfs.core.windows.net/"
		},
		"ls_synapse_udacitynycpayroll_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "udacity-nyc-payroll.database.windows.net"
		},
		"ls_synapse_udacitynycpayroll_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "udacity"
		},
		"ls_synapse_udacitynycpayroll_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-nyc-payroll_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_masterdata_accountKey": {
						"value": "[parameters('ls_adls_masterdata_accountKey')]"
					},
					"ls_sqldb_nycpayroll_connectionString": {
						"value": "[parameters('ls_sqldb_nycpayroll_connectionString')]"
					},
					"ls_synapse_udacitynycpayroll_password": {
						"value": "[parameters('ls_synapse_udacitynycpayroll_password')]"
					},
					"ls_adls_masterdata_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_masterdata_properties_typeProperties_url')]"
					},
					"ls_synapse_udacitynycpayroll_properties_typeProperties_server": {
						"value": "[parameters('ls_synapse_udacitynycpayroll_properties_typeProperties_server')]"
					},
					"ls_synapse_udacitynycpayroll_properties_typeProperties_database": {
						"value": "[parameters('ls_synapse_udacitynycpayroll_properties_typeProperties_database')]"
					},
					"ls_synapse_udacitynycpayroll_properties_typeProperties_userName": {
						"value": "[parameters('ls_synapse_udacitynycpayroll_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-nyc-payroll_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-nyc-payroll_ArmTemplate_0')]"
			]
		}
	]
}